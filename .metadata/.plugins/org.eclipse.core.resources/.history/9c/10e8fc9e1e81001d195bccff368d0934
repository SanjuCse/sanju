package whatsTheOutput;

public class CallGlobalVariablesWithConstructor {
	int a, b;

	public CallGlobalVariablesWithConstructor(int x, int y) {
		a = x;
		b = y;
	}

	public static void main(String[] args) {
	CallGlobalVariablesWithConstructor withConstructor = new CallGlobalVariablesWithConstructor();
	System.out.println(withConstructor.a+ "" +withConstructor.b);

	}

}
