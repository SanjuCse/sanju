package com.jtp.java.aggregation;

class Operation {
	int square(int radius) {
		return radius * radius;
	}
}

class Circle {
	double pi = 3.14;
	Operation operation;

	double area(int radius) {
		operation = new Operation();
		int Square = operation.square(radius);
		return pi;
	}
}

public class AggregationEx {

}
